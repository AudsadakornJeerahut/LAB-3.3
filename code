#include <stdio.h>

struct student {
 	char name[ 20 ] ;
 	int age ;
 	char sex ;
 	float gpa ;
} ;

struct student upgrade( struct student child ) ;

int main() {
struct student aboy ;
    struct student agirl ;
    
    aboy.sex = 'M' ;
    aboy.gpa = 3.00 ;
    agirl.sex = 'W' ;
    agirl.gpa = 3.00 ;
    
    aboy = upgrade( aboy ) ;
    agirl = upgrade( agirl ) ;
    
    printf( "GPA[M] = %.2f \n", aboy.gpa ) ;
    printf( "GPA[W] = %.2f", agirl.gpa ) ;
 	return 0 ;
}//end function

struct student upgrade( struct student child ) {
	
	if ( child.sex == 'M' ) {
		child.gpa = child.gpa * ( 1 + 10 / 100.0 ) ;
	} else if ( child.sex == 'W' ) {
		child.gpa = child.gpa * ( 1 + 20 / 100.0 ) ;
	}
	return child ;
}
